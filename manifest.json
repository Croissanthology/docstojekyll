={
  "manifest_version": 3,
  "name": "GDocs to Jekyll",
  "version": "1.0",
  "description": "convert google docs to jekyll posts with one click",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://docs.google.com/*",
    "https://api.github.com/*"
  ],
  "content_scripts": [{
    "matches": ["https://docs.google.com/document/*"],
    "js": ["parser.js"]
  }],
  "action": {
    "default_popup": "src/popup/popup.html"
  }
}
